### 2.第二题：
def my_num(*ret):
    count = {}
    for i in ret:
        count[i]= ret.count(i)
    return count

rest1  = my_num('ab', 'ab', 'c' ,'cccc', 'ccc' ,'cccc')
print(rest1)
